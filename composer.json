{
    "name": "tests",
    "require": {
        "guzzlehttp/guzzle": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "App\\Commands\\": "app/commands/",
            "App\\Jobs\\": "app/jobs/",
            "App\\Services\\": "app/Services/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
